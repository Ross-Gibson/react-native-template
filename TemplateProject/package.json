{
  "name": "TemplateProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "tslint -c tslint.json 'app/**/*.{ts,tsx}'",
    "test": "bin/test"
  },
  "dependencies": {
    "react": "16.0.0-alpha.12",
    "react-native": "0.45.0",
    "react-native-config": "^0.3.1",
    "react-navigation": "1.0.0-beta.8",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@types/jest": "^19.2.2",
    "@types/react": "^15.0.22",
    "@types/react-native": "^0.43.6",
    "@types/react-redux": "^4.4.39",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "babel-jest": "^20.0.3",
    "babel-preset-react-native": "^1.9.2",
    "jest": "^20.0.4",
    "react-native-typescript-transformer": "^1.0.9",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-mock-store": "^1.2.2",
    "remote-redux-devtools": "^0.5.7",
    "ts-jest": "^20.0.6",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4"
  },
  "jest": {
    "preset": "react-native",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(j|t)sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-navigation|react-clone-referenced-element|expo|@expo/*|exponent|@exponent/*)"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ]
  }
}
